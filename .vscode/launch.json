    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "name": "Run OCN Node (local-minikube)",
            "request": "launch",
            "runtimeExecutable": "bash",
            "runtimeArgs": [
                "-c",
                "JAVA_HOME=\"/Users/matheusrosendo/Library/Java/JavaVirtualMachines/azul-13.0.14/Contents/Home\" SPRING_PROFILES_ACTIVE=local-minikube ./gradlew bootRun -Pprofile=local-minikube -Dorg.gradle.jvmargs='--add-opens=jdk.compiler/com.sun.tools.javac.main=ALL-UNNAMED --add-opens=jdk.compiler/com.sun.tools.javac.processing=ALL-UNNAMED --add-opens=jdk.compiler/com.sun.tools.javac.util=ALL-UNNAMED'"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "name": "Run OCN Node with Debug (local-minikube)",
            "request": "launch",
            "runtimeExecutable": "bash",
            "runtimeArgs": [
                "-c",
                "JAVA_HOME=\"/Users/matheusrosendo/Library/Java/JavaVirtualMachines/azul-13.0.14/Contents/Home\" SPRING_PROFILES_ACTIVE=local-minikube ./gradlew bootRun -Pprofile=local-minikube -Dorg.gradle.jvmargs='-agentlib:jdwp=transport=dt_socket,server=y,suspend=n,address=*:5005 --add-opens=jdk.compiler/com.sun.tools.javac.main=ALL-UNNAMED --add-opens=jdk.compiler/com.sun.tools.javac.processing=ALL-UNNAMED --add-opens=jdk.compiler/com.sun.tools.javac.util=ALL-UNNAMED'"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
        {
            "type": "java",
            "name": "Debug (Attach to Running App)",
            "request": "attach",
            "hostName": "localhost",
            "port": 5005
        }
    ]
}